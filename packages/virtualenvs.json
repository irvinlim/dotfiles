{
  "default": "python3",
  "venvs": {
    "base": {
      "interpreter": "python3",
      "requirements": ["base"]
    },
    "base2.7": {
      "interpreter": "python2.7",
      "requirements": ["base"]
    },
    "python2.7": {
      "interpreter": "python2.7",
      "requirements": ["base", "python2", "tools"]
    },
    "python3": {
      "interpreter": "python3",
      "requirements": ["base", "python3", "tools"]
    }
  },
  "pythonPaths": {
    "python2.7": [
      "/usr/local/bin/python2.7",
      "/usr/bin/python2.7"
    ],
    "python3": [
      "/usr/local/bin/python3",
      "/usr/bin/python3"
    ]
  }
}
