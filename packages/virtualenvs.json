{
  "default": "python3.7",
  "venvs": {
    "base": {
      "interpreter": "python3.7",
      "requirements": ["base"]
    },
    "python2.7": {
      "interpreter": "python2.7",
      "requirements": ["base", "python2", "tools"]
    },
    "python3.7": {
      "interpreter": "python3.7",
      "requirements": ["base", "python3", "tools"]
    },
    "ctf": {
      "interpreter": "python2.7",
      "requirements": ["base", "python2", "tools", "ctf"]
    },
    "ds": {
      "interpreter": "python3.7",
      "requirements": ["base", "python3", "tools", "ds"]
    }
  },
  "pythonPaths": {
    "python2.7": ["/usr/local/bin/python2.7", "/usr/bin/python2.7"],
    "python3.7": [
      "~/anaconda3/bin/python3.7",
      "/usr/local/bin/python3.7",
      "/usr/bin/python3.7"
    ]
  }
}
