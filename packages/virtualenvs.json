{
  "default": "python3",
  "venvs": {
    "base": {
      "interpreter": "python3",
      "requirements": ["base"]
    },
    "python2": {
      "interpreter": "python2",
      "requirements": ["base", "python2", "tools"]
    },
    "python3": {
      "interpreter": "python3",
      "requirements": ["base", "python3", "tools"]
    }
  },
  "pythonPaths": {
    "python2": ["/usr/local/bin/python2.7", "/usr/bin/python2.7"],
    "python2.7": ["/usr/local/bin/python2.7", "/usr/bin/python2.7"],
    "python3": [
      "/usr/local/bin/python3.7",
      "/usr/bin/python3.7",
      "/usr/local/bin/python3.6",
      "/usr/bin/python3.6"
    ],
    "python3.6": ["/usr/local/bin/python3.6", "/usr/bin/python3.6"],
    "python3.7": ["/usr/local/bin/python3.7", "/usr/bin/python3.7"]
  }
}
